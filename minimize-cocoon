    [25, 40],
    [26, 40],
    [27, 40],
    [53, 40],
    [54, 40],
    [55, 40],
    [56, 40],
    [57, 40],
    [58, 40],
    [59, 40],
    [60, 40],
    [61, 40],
    [62, 40],
    [63, 40],
    [64, 40],
    [65, 40],
    [66, 40],
    [67, 40],
    [68, 40],
;

my $TOTAL = @camel-coords.elems;
die "Unequal size arrays"
    unless $TOTAL == @cocoon-coords.elems;

my @mapping = (^$TOTAL).pick(*);

sub dist(@mapping, $i) {
    my @cc := @camel-coords[$i];
    my @bc := @cocoon-coords[ @mapping[$i] ];

    my $dx = @bc[0] - @cc[0];
    my $dy = @bc[1] - @cc[1];

    return $dx * $dx + $dy * $dy;
}

sub total_dist(@mapping) {
    my $sum = 0;
    for ^$TOTAL -> $i {
        $sum += dist(@mapping, $i);
    }
    return $sum;
}

my $sum = total_dist(@mapping);
my $attempts = 0;
until (now - BEGIN now) > 1200 {
    my ($i1, $i2);
    repeat {
        ($i1, $i2) = (^$TOTAL).roll, (^$TOTAL).roll;
    } until $i1 != $i2;
    my $d_old = dist(@mapping, $i1) + dist(@mapping, $i2);
    my @new_mapping = @mapping;
    @new_mapping[$i1, $i2] = @new_mapping[$i2, $i1];
    my $d_new = dist(@new_mapping, $i1) + dist(@new_mapping, $i2);
    my $temperature = (600 + BEGIN { now } - now) / 60;
    if $d_new < $d_old + rand * $temperature {
        @mapping = @new_mapping;
        $sum += $d_new - $d_old;
        $attempts = 0;
    }
    else {
        $attempts++;
    }
}

my $file = "run-cocoon-1";
$file++ while $file.IO ~~ :e;
spurt $file, $sum ~ "\n\n" ~ join "\n", @mapping;
